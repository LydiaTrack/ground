DB_URI=mongodb://localhost:27017
DB_NAME=renoten-test
DB_PORT=27017
# It defines the DB type for mongoDB, it can be either CONTAINER or REMOTE
DB_CONNECTION_TYPE=CONTAINER
# JWT Configuration
JWT_SECRET=your_jwt_secret_here_make_it_long_and_random
JWT_EXPIRES_IN_MINUTES=60
JWT_REFRESH_EXPIRES_IN_HOUR=72

# Default User Configuration
DEFAULT_USER_USERNAME=admin
DEFAULT_USER_PASSWORD=change_this_password
DEFAULT_ROLE_NAME=ADMIN
DEFAULT_ROLE_TAGS=ADMIN_ROLE
DEFAULT_ROLE_INFO=Administrator role

# OAuth Configuration (Optional)
# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:8080/auth/google/callback

# Apple OAuth
APPLE_CLIENT_ID=your_apple_client_id
APPLE_TEAM_ID=your_apple_team_id
APPLE_KEY_ID=your_apple_key_id
APPLE_PRIVATE_KEY=your_apple_private_key_content
APPLE_REDIRECT_URI=http://localhost:8080/auth/apple/callback

# Email Configuration (Optional)
EMAIL_TYPE_RESET_PASSWORD_ADDRESS=your_email@example.com
EMAIL_TYPE_RESET_PASSWORD_PASSWORD=your_email_password

# Environment Type
ENV_TYPE=development
